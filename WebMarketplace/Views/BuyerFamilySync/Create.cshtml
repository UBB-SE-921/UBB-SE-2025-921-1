@model WebMarketplace.Models.BuyerFamilySyncViewModel

@{
    ViewData["Title"] = "Create Buyer Family Sync";
}

<h1>Create Buyer Family Sync</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="LinkedBuyerName" class="control-label"></label>
        <input asp-for="LinkedBuyerName" class="form-control" />
        <span asp-validation-for="LinkedBuyerName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Status" class="control-label"></label>
        <input asp-for="Status" class="form-control" />
        <span asp-validation-for="Status" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
